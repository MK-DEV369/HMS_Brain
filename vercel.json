{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
      "src": "hms_backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/eeg_app/$1"
    }
  ],
  "functions": {
    "backend/eeg_app": {
      "includeFiles": "backend/intelligence/preprocessed/**/*"
    }
  },
  "env": {
    "ENVIRONMENT": "production",
    "VERCEL": "true",
    "DEBUG": "false"
  }
}
